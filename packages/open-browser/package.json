{
  "name": "@stryke/open-browser",
  "version": "0.2.24",
  "type": "module",
  "description": "A package containing helper function to open URLs in the default browser",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke.git",
    "directory": "packages/open-browser"
  },
  "private": false,
  "files": ["openChrome.applescript"],
  "dependencies": {
    "cross-spawn": "catalog:"
  },
  "devDependencies": {
    "@types/cross-spawn": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./open": {
      "require": "./dist/open.cjs",
      "import": "./dist/open.mjs"
    },
    "./*": "./*"
  }
}
