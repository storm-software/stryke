{
  "name": "@stryke/hash",
  "version": "0.12.32",
  "type": "module",
  "description": "A package containing utility functions that hash data using various algorithms.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke.git",
    "directory": "packages/hash"
  },
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./digest": {
      "require": "./dist/digest.cjs",
      "import": "./dist/digest.mjs"
    },
    "./etag": {
      "require": "./dist/etag.cjs",
      "import": "./dist/etag.mjs"
    },
    "./hash-files": {
      "require": "./dist/hash-files.cjs",
      "import": "./dist/hash-files.mjs"
    },
    "./md5": {
      "require": "./dist/md5.cjs",
      "import": "./dist/md5.mjs"
    },
    "./murmurhash": {
      "require": "./dist/murmurhash.cjs",
      "import": "./dist/murmurhash.mjs"
    },
    "./neutral": {
      "require": "./dist/neutral.cjs",
      "import": "./dist/neutral.mjs"
    },
    "./xx-hash": {
      "require": "./dist/xx-hash.cjs",
      "import": "./dist/xx-hash.mjs"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.cts",
  "dependencies": {
    "js-xxhash": "catalog:",
    "ohash": "catalog:"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
