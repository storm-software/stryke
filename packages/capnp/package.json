{
  "name": "@stryke/capnp",
  "version": "0.12.39",
  "type": "module",
  "description": "A package to assist in running the Cap'n Proto compiler and creating Cap'n Proto serialization protocol schemas.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke",
    "directory": "packages/capnp"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://stormsoftware.com/support",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "bin": {
    "storm-capnpc": "bin/capnpc.mjs",
    "storm-capnpc-cjs": "bin/capnpc.cjs",
    "storm-capnpc-esm": "bin/capnpc.mjs"
  },
  "main": "./dist/src/index.cjs",
  "module": "./dist/src/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/src/index.cjs",
      "import": "./dist/src/index.mjs"
    },
    "./compile": {
      "require": "./dist/src/compile.cjs",
      "import": "./dist/src/compile.mjs"
    },
    "./cpp": {
      "require": "./dist/schemas/cpp.cjs",
      "import": "./dist/schemas/cpp.mjs"
    },
    "./helpers": {
      "require": "./dist/src/helpers.cjs",
      "import": "./dist/src/helpers.mjs"
    },
    "./persistent": {
      "require": "./dist/schemas/persistent.cjs",
      "import": "./dist/schemas/persistent.mjs"
    },
    "./rpc": {
      "require": "./dist/schemas/rpc.cjs",
      "import": "./dist/schemas/rpc.mjs"
    },
    "./rpc-twoparty": {
      "require": "./dist/schemas/rpc-twoparty.cjs",
      "import": "./dist/schemas/rpc-twoparty.mjs"
    },
    "./schema": {
      "require": "./dist/schemas/schema.cjs",
      "import": "./dist/schemas/schema.mjs"
    },
    "./ts": {
      "require": "./dist/schemas/ts.cjs",
      "import": "./dist/schemas/ts.mjs"
    },
    "./types": {
      "require": "./dist/src/types.cjs",
      "import": "./dist/src/types.mjs"
    },
    "./*": "./*"
  },
  "types": "./dist/src/index.d.cts",
  "files": ["bin/**/*", "dist/**/*", "dts/**/*", "schemas/**/*", "vendor/**/*"],
  "keywords": [
    "capnp",
    "capnproto",
    "serialization",
    "protocol",
    "compiler",
    "storm-software",
    "stryke"
  ],
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": false
    }
  },
  "dependencies": {
    "@stryke/fs": "workspace:*",
    "@stryke/path": "workspace:*",
    "defu": "catalog:",
    "hex2dec": "catalog:",
    "nanotar": "catalog:"
  },
  "devDependencies": {
    "@storm-software/config": "catalog:",
    "@storm-software/config-tools": "catalog:",
    "@storm-software/testing-tools": "catalog:",
    "@stryke/tools-config": "workspace:*",
    "@types/node": "catalog:",
    "capnp-es": "catalog:",
    "chalk": "catalog:",
    "commander": "catalog:",
    "dts-bundle-generator": "catalog:",
    "tsdown": "catalog:",
    "tsx": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
