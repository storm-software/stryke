{
  "name": "@stryke/crypto",
  "version": "0.5.22",
  "type": "module",
  "description": "A package containing cryptographic utilities used by Storm Software.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke.git",
    "directory": "packages/crypto"
  },
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./base-64": {
      "require": "./dist/base-64.cjs",
      "import": "./dist/base-64.mjs"
    },
    "./encryption": {
      "require": "./dist/encryption.cjs",
      "import": "./dist/encryption.mjs"
    },
    "./encryption.node": {
      "require": "./dist/encryption.node.cjs",
      "import": "./dist/encryption.node.mjs"
    },
    "./hex": {
      "require": "./dist/hex.cjs",
      "import": "./dist/hex.mjs"
    },
    "./neutral": {
      "require": "./dist/neutral.cjs",
      "import": "./dist/neutral.mjs"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.cts",
  "dependencies": {
    "@stryke/convert": "workspace:*",
    "@stryke/type-checks": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
