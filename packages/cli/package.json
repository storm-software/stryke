    },
    "./utils/strip-ansi": {
      "require": "./dist/utils/strip-ansi.cjs",
      "import": "./dist/utils/strip-ansi.mjs"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.cts",
  "dependencies": {
    "@antfu/ni": "catalog:",
    "defu": "catalog:",
    "execa": "catalog:",
    "figlet": "catalog:",
    "is-unicode-supported": "catalog:",
    "string-width": "catalog:",
    "terminal-link": "catalog:"
  },
  "devDependencies": {
    "@types/figlet": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
