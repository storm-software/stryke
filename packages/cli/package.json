{
  "name": "@stryke/cli",
  "version": "0.12.26",
  "type": "module",
  "description": "A package containing various CLI helper functions",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke.git",
    "directory": "packages/cli"
  },
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./create-options": {
      "require": "./dist/create-options.cjs",
      "import": "./dist/create-options.mjs"
    },
    "./define-command": {
      "require": "./dist/define-command.cjs",
      "import": "./dist/define-command.mjs"
    },
    "./execute": {
      "require": "./dist/execute.cjs",
      "import": "./dist/execute.mjs"
    },
    "./get-command": {
      "require": "./dist/get-command.cjs",
      "import": "./dist/get-command.mjs"
    },
    "./meta": {
      "require": "./dist/meta.cjs",
      "import": "./dist/meta.mjs"
    },
    "./parse": {
      "require": "./dist/parse.cjs",
      "import": "./dist/parse.mjs"
    },
    "./run": {
      "require": "./dist/run.cjs",
      "import": "./dist/run.mjs"
    },
    "./shutdown": {
      "require": "./dist/shutdown.cjs",
      "import": "./dist/shutdown.mjs"
    },
