      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./usage": {
      "require": "./dist/usage.cjs",
      "import": "./dist/usage.mjs"
    },
    "./utils": {
      "require": "./dist/utils/index.cjs",
      "import": "./dist/utils/index.mjs"
