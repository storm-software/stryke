{
  "name": "@stryke/path",
  "version": "0.23.2",
  "type": "module",
  "description": "A package containing various utilities that expand the functionality of NodeJs's built-in `path` module",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/stryke.git",
    "directory": "packages/path"
  },
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./append": {
      "require": "./dist/append.cjs",
      "import": "./dist/append.mjs"
    },
    "./asset-extensions": {
      "require": "./dist/asset-extensions.cjs",
      "import": "./dist/asset-extensions.mjs"
    },
    "./common": {
      "require": "./dist/common.cjs",
      "import": "./dist/common.mjs"
    },
    "./correct-path": {
      "require": "./dist/correct-path.cjs",
      "import": "./dist/correct-path.mjs"
    },
    "./cwd": {
      "require": "./dist/cwd.cjs",
      "import": "./dist/cwd.mjs"
    },
    "./delimiter": {
      "require": "./dist/delimiter.cjs",
      "import": "./dist/delimiter.mjs"
    },
    "./file-path-fns": {
      "require": "./dist/file-path-fns.cjs",
      "import": "./dist/file-path-fns.mjs"
    },
    "./find": {
      "require": "./dist/find.cjs",
      "import": "./dist/find.mjs"
    },
    "./is-parent-path": {
      "require": "./dist/is-parent-path.cjs",
      "import": "./dist/is-parent-path.mjs"
    },
    "./is-root-dir": {
      "require": "./dist/is-root-dir.cjs",
      "import": "./dist/is-root-dir.mjs"
    },
    "./is-type": {
      "require": "./dist/is-type.cjs",
      "import": "./dist/is-type.mjs"
    },
    "./join": {
      "require": "./dist/join.cjs",
      "import": "./dist/join.mjs"
    },
    "./join-paths": {
      "require": "./dist/join-paths.cjs",
      "import": "./dist/join-paths.mjs"
    },
    "./normalize": {
      "require": "./dist/normalize.cjs",
      "import": "./dist/normalize.mjs"
    },
    "./regex": {
      "require": "./dist/regex.cjs",
      "import": "./dist/regex.mjs"
    },
    "./replace": {
      "require": "./dist/replace.cjs",
      "import": "./dist/replace.mjs"
    },
    "./resolve-parent-path": {
      "require": "./dist/resolve-parent-path.cjs",
      "import": "./dist/resolve-parent-path.mjs"
    },
    "./slash": {
      "require": "./dist/slash.cjs",
      "import": "./dist/slash.mjs"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.cts",
  "devDependencies": {
    "@storm-software/testing-tools": "catalog:",
    "@stryke/convert": "workspace:*",
    "@stryke/type-checks": "workspace:*",
    "@stryke/types": "workspace:*",
    "tsdown": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
